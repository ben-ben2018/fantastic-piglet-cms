{
  "kind": "collectionType",
  "collectionName": "jj_contents",
  "info": {
    "singularName": "jj-content",
    "pluralName": "jj-contents",
    "displayName": "JjContent",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext",
      "required": false
    },
    "title": {
      "type": "text"
    },
    "like": {
      "type": "integer"
    },
    "views": {
      "type": "integer"
    },
    "tags": {
      "type": "json",
      "required": true
    },
    "author_id": {
      "type": "integer"
    },
    "hot": {
      "type": "float",
      "default": 0
    },
    "jj_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::jj-user.jj-user"
    },
    "summary": {
      "type": "text"
    },
    "tag": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tag.tag"
    },
    "ad": {
      "type": "boolean",
      "default": false,
      "required": false
    }
  }
}
